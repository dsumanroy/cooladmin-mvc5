<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Dashboard</title>

    <!-- Fontfaces CSS-->
    <link href="@Url.Content("~/Content/font-face.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/font-awesome-4.7/css/font-awesome.min.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/vendor/font-awesome-5/css/fontawesome-all.min.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/vendor/mdi-font/css/material-design-iconic-font.min.css")" rel="stylesheet" type="text/css" media="all" />


    <!-- Bootstrap CSS-->
    <link href="" media="all">
    <link href="@Url.Content("~/vendor/bootstrap-4.1/bootstrap.min.css")" rel="stylesheet" type="text/css" media="all" />
    <!-- Vendor CSS-->
    <link href="@Url.Content("~/vendor/animsition/animsition.min.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/wow/animate.css")" rel="stylesheet" type="text/css" media="all" />


    <link href="@Url.Content("~/vendor/css-hamburgers/hamburgers.min.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/slick/slick.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/select2/select2.min.css")" rel="stylesheet" type="text/css" media="all" />

    <link href="@Url.Content("~/vendor/perfect-scrollbar/perfect-scrollbar.css")" rel="stylesheet" type="text/css" media="all" />

    <!-- Main CSS-->

    <link href="@Url.Content("~/Content/theme.css")" rel="stylesheet" type="text/css" media="all" />
</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <header class="header-mobile d-block d-lg-none">
            <div class="header-mobile__bar">
                <div class="container-fluid">
                    <div class="header-mobile-inner">
                        <a class="logo" href="~/">
                            <img class="navbar-logo" src="@Url.Content("~/images/icon/logo.png")" alt="CoolAdmin">                            
                        </a>
                        <button class="hamburger hamburger--slider" type="button">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>

            @Html.Partial("_MobNav")

        </header>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="~/">
                    <img class="navbar-logo" src="@Url.Content("~/images/icon/logo.png")" alt="Cool Admin">
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                @Html.Partial("_LeftNav")
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            @Html.Partial("_Header")
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()

            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="@Url.Content("~/vendor/jquery-3.2.1.min.js")" type="text/javascript"></script>

    <!-- Bootstrap JS-->
    <script src="@Url.Content("~/vendor/bootstrap-4.1/popper.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/vendor/bootstrap-4.1/bootstrap.min.js")" type="text/javascript"></script>

    <!-- Vendor JS       -->

    <script src="@Url.Content("~/vendor/slick/slick.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/vendor/wow/wow.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/vendor/animsition/animsition.min.js")" type="text/javascript"></script>


    <script src="@Url.Content("~/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/vendor/counter-up/jquery.waypoints.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/vendor/counter-up/jquery.counterup.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/vendor/circle-progress/circle-progress.min.js")" type="text/javascript"></script>



    <script src="@Url.Content("~/vendor/perfect-scrollbar/perfect-scrollbar.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/vendor/chartjs/Chart.bundle.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/vendor/select2/select2.min.js")" type="text/javascript"></script>

    <!-- Main JS-->
    <script src="@Url.Content("~/Scripts/main.js")" type="text/javascript"></script>
</body>

</html>
<!-- end document-->
